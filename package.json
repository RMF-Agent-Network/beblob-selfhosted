    "webpack-dev-server": "^4.8.0",
    "marked": "^12.0.1",
    "marked-highlight": "^2.1.1",
    "highlight.js": "^11.9.0",
    "simplemde": "^1.11.2",
    "codemirror": "5.65.16"
  },
